{
   "compilerOptions": {
     "lib": [
       "dom",
       "dom.iterable",
       "ESNext"
     ],
     "allowJs": false,
     "jsx": "react-jsx",
     "noEmit": true,
     "strict": true,
     "moduleResolution": "node",
     "resolveJsonModule": true,
     "isolatedModules": true,
     "allowSyntheticDefaultImports": true,
     "esModuleInterop": true,
     "typeRoots": [
       "./src/@types"
     ],
     "skipLibCheck": true,
     "forceConsistentCasingInFileNames": true,
     "target": "es5",
     "noFallthroughCasesInSwitch": true,
     "module": "esnext"
   },
   "include": ["src"]
   ,
   "exclude": [
     "node_modules"
   ]
 }